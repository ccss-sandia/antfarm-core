#! /usr/bin/ruby

# require File.dirname(__FILE__) + '/../config/boot'
require File.dirname(__FILE__) + '/../config/environment'

require 'antfarm/plugin'

plugins = Array.new
Antfarm::Plugin.discover do |plugin|
  plugins << plugin
end

puts plugins.sort

Antfarm::Plugin.load('viz/display_networks') do |plugin|
  puts plugin.name
end

DataMapper.auto_migrate!
